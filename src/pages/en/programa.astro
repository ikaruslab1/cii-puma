          </div>

          <ActEn titulo="Parallel talk (ROOM A)" />
          <ActEn titulo="Parallel talk (ROOM B)" />
          <ActEn titulo="Parallel talk (ROOM C)" />
          <ActEn titulo="Parallel talk (ROOM D)" ultimo={true} />

          <!-- Martes -->
        </div>
      </div>
      <div
        id="tabs-fill-3"
        class="hidden"
        role="tabpanel"
        aria-labelledby="tabs-fill-item-3"
      >
        <div class="grid grid-cols-2 px-4 md:px-16 xl:px-64">
          <!-- Miércoles -->

          <ActEn
            tag={Eje3}
